<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="810.0" prefWidth="1028.0" style="-fx-background-color: #9EDDFF;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.controller.ForgetPWController">
   <children>
      <Label layoutX="292.0" layoutY="152.0" prefHeight="112.0" prefWidth="443.0" text="If you forgot your password, Then click the button send OTP and enter the below text field that received pin on your email. Then you can change your password." wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="292.0" layoutY="264.0" mnemonicParsing="false" onAction="#btnSendOtp" prefHeight="31.0" prefWidth="120.0" stylesheets="@styles.css" text="Send OTP" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="txtOtp" layoutX="299.0" layoutY="315.0" prefHeight="11.0" prefWidth="142.0" promptText="XXXX" style="-fx-background-color: #E3E5DF;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button layoutX="465.0" layoutY="315.0" mnemonicParsing="false" onAction="#btnOtpConfirm" prefHeight="37.0" prefWidth="98.0" stylesheets="@styles.css" text="Confirm" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="txtEnterPw" layoutX="299.0" layoutY="389.0" prefHeight="31.0" prefWidth="373.0" promptText="ENTER PASSWORD" style="-fx-background-color: #E3E5DF;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="txtConfirmPw" layoutX="299.0" layoutY="450.0" prefHeight="24.0" prefWidth="373.0" promptText="CONFIRM PASSWORD" style="-fx-background-color: #E3E5DF;">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <CheckBox layoutX="554.0" layoutY="509.0" mnemonicParsing="false" onAction="#chechShowPw" prefWidth="120.0" text="Show Password" />
      <Button layoutX="541.0" layoutY="561.0" mnemonicParsing="false" onAction="#btnAllConfirm" prefHeight="37.0" prefWidth="128.0" stylesheets="@styles.css" text="Confirm" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
